actor Lith_Revenant : Revenant replaces Revenant
{
   Health 350
   Tag "$LITH_INFO_SHORT_Revenant"
   
   states
   {
   Death:
      "####" "#" 0 ACS_NamedExecuteWithResult("Lith_EmitScore", Score_Revenant)
      goto Super::Death
   XDeath:
      REVX A 3
      REVX B 4 A_XScream
      REVX C 5 A_NoBlocking
      REVX DE 5
      REVX F -1
      stop
   }
}

