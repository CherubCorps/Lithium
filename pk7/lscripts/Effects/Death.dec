actor Lith_DeathParticleSmall
{
   RenderStyle "Subtract"
// Alpha 0.5
   Radius 3
   Height 3
   Scale 0.2
   
   +THRUACTORS
   
   States
   {
   Spawn:
      TNT1 A 0
      TNT1 A 0 A_SetScale(scalex * frandom(0.08, 0.14))
