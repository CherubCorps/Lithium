actor Lith_ScoreCount : Inventory
{
   Inventory.MaxAmount 0x7FFFFFFF
}

actor Lith_ScoreItem : CustomInventory
{
   Inventory.PickupMessage ""
   Inventory.PickupSound ""
   +INVENTORY.TRANSFER
   
   states
   {
   Pickup:
      TNT1 A 0 ACS_NamedExecuteAlways("Lith_UpdateScore")
      stop
   }
}

// EOF

